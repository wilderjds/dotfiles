
# ~/.XCompose
# This file defines custom Compose sequences for Unicode characters

# Import default rules from the system Compose file:
include "/usr/share/X11/locale/en_US.UTF-8/Compose"

include "%H/.XCompose.d/xcompose.greek"
include "%H/.XCompose.d/xcompose.math"

# To put some stuff onto compose key strokes:
<Multi_key> <minus> <greater> : "‚Üí" U2192 # Compose - >
<Multi_key> <minus> <less> : "‚Üê" U2190
<Multi_key> <equal> <greater> : "‚áí" U21D2
<Multi_key> <equal> <less> : "‚áê" U21D0
<Multi_key> <minus> <asciicircum> : "‚Üë" U2191
<Multi_key> <minus> <v> : "‚Üì" U2193

<Multi_key> <less> <underscore> : "‚â§" U2264
<Multi_key> <greater> <underscore> : "‚â•" U2265

<Multi_key> <v> <v> : "‚úì" U2713

<Multi_key> <backslash> <o> <slash>            : "üôå"               # PERSON RAISING BOTH HANDS IN CELEBRATION

<Multi_key> <grave> <grave>       : "‚Äú"   U201c # LEFT DOUBLE QUOTATION MARK
<Multi_key> <acute> <acute>       : "‚Äù"   U201d # RIGHT DOUBLE QUOTATION MARK

#Abbrev_it
<Multi_key> <c> <m> <q> : "comunque"
<Multi_key> <p> <w> <d> : "password"

#Abbrev
<Multi_key> <o> <t> <o> <h> : "on the other hand"
<Multi_key> <O> <t> <o> <h> : "On the other hand"

<Multi_key> <less> <3> : "‚ù§"

# reversed accents
